<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boot.store.orderList.dao.OrderListDao">
	
	<insert id="addOrderList" parameterType="orderList">
		<selectKey keyProperty="o_num" resultType="int" order="BEFORE">
			select max(o_num) as o_num from orders
		</selectKey>
		insert into order_list
		values(SEQ_ORDER_LIST.nextval, #{o_num}, #{m_num}, #{i_num}, #{ol_quan})
	</insert>
</mapper>